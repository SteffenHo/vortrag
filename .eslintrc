{
  "env": {
    "browser": true,
    "jquery": false,
    "es6": true
  },
  "globals": {
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4],
    "react/forbid-prop-types": ["error", {
      "forbid": ["any", "array"]
    }]
  },
  "extends": "airbnb"
}
